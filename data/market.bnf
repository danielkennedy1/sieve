<strategy> ::= <condition> ? ( "BUY" ) : ( <condition> ? ( "SELL" ) : ( "HOLD" ) )
<condition> ::= <expr> <comp> <expr> | true | false
<comp> ::= > | < | ==
<expr> ::= ( <condition> ? <expr> : <expr> ) | ( <expr> <op> <expr> ) | ( <val> )
<op> ::= + | - | * | /
<val> ::= $PRICE | <const>
<const> ::= 400..500
