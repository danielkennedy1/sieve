<strategy> ::= <condition> ? ( "BUY" ) : ( <condition> ? ( "SELL" ) : ( "HOLD" ) )
<condition> ::= <numeric> <comp> <numeric>
<comp> ::= > | < | ==
<numeric> ::= ( <condition> ? <numeric> : <numeric> ) | ( <numeric> <op> <numeric> ) | ( <val> )
<op> ::= + | - | * | /
<val> ::= $PRICE | $HOLDINGS | <const>
<const> ::= 0..500
