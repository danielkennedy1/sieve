<strategy> ::= IF <condition> BUY <proportion> AT <price> ELSE IF <condition> SELL <proportion> AT <price>
<price> ::= "MARKET"
<condition> ::= <expr> <comp> <expr> | true | false
<comp> ::= > | < | ==
<expr> ::= if <condition> then <expr> else <expr> | <expr> <op> <expr> | <expr> <op> <expr> | <val> | <val>
<op> ::= + | - | * | /
<val> ::= price[ <ago> ] | <const>
<const> ::= 1 | 2 | 3 | 4 | 5
<ago> ::= 1 | 2 | 3
