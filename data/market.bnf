<strategy> ::= IF <condition> BUY AT <price> ELSE IF <condition> SELL AT <price>
<price> ::= "MARKET"
<condition> ::= <expr> <comp> <expr> | true | false
<comp> ::= > | < | ==
<expr> ::= if <condition> then <expr> else <expr> | <expr> <op> <expr> | <expr> <op> <expr> | <val> | <val>
<op> ::= + | - | * | /
<val> ::= price | <const>
<const> ::= 400..500
