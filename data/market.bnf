<strategy> ::= <condition> ? ( "SELL" ) : ( <condition> ? ( "BUY" ) : ( "HOLD" ) )
<condition> ::= <val> <comp> <const>
<comp> ::= > | < | ==
<numeric> ::= ( <condition> ? <numeric> : <numeric> ) | ( <numeric> <op> <numeric> ) | ( <val> )
<op> ::= + | - | * | /
<val> ::= $PRICE | $RSI
<proportion> ::= 0.0..1.0
<const> ::= 0..100
